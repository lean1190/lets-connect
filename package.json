{
  "name": "lets-connect",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=11.6.1",
    "node": "24.x"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "debug": "next dev --inspect",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "test": "vitest run --coverage --passWithNoTests",
    "types:check": "tsc --skipLibCheck --noEmit",
    "supabase:types": "source .env.local && npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/database/types.ts",
    "supabase:login": "npx supabase login",
    "generate:splash": "tsx scripts/generate-splash-images.ts",
    "generate:icons": "tsx scripts/generate-icons.ts",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu --interactive && npm install && npm run lint",
    "upgrade:next": "npx @next/codemod",
    "skills": "SKILLS_NO_TELEMETRY=1 && npx skills add vercel-labs/agent-skills",
    "analyze": "next experimental-analyze",
    "prepare": "husky"
  },
  "dependencies": {
    "@date-fns/utc": "2.1.1",
    "@hookform/resolvers": "5.2.2",
    "@next-safe-action/adapter-react-hook-form": "2.0.0",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-slot": "1.2.4",
    "@supabase/ssr": "0.8.0",
    "@supabase/supabase-js": "2.90.1",
    "@tabler/icons-react": "3.36.1",
    "@vercel/analytics": "1.6.1",
    "@vercel/speed-insights": "1.3.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "next": "16.1.3",
    "next-safe-action": "8.0.11",
    "qr-scanner": "^1.4.2",
    "qrcode.react": "4.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "recharts": "3.6.0",
    "tailwind-merge": "3.4.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.17",
    "babel-plugin-react-compiler": "1.0.0",
    "husky": "9.1.7",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.7",
    "next-pwa": "5.6.0",
    "npm-check-updates": "19.3.1",
    "sharp": "^0.34.5",
    "supabase": "2.72.8",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "1.4.0",
    "typescript": "^5",
    "vitest": "4.0.17"
  }
}
